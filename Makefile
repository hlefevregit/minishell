# Clean all .o and .a files
clean:
	@printf $(CYAN)"\r\033[KErasing objects... "$(RESET)"‚è≥ "
	@rm -rdf $(OBJ)
	@make -C lib/libft clean
	@make -C lib/get_next_line clean
	@printf $(GREEN)"\r\033[Kcleaned üóë"$(RESET)

# Same as 'clean' but clean minishell too
fclean:
	@printf $(CYAN)"\r\033[KErasing objects... "$(RESET)"‚è≥ "
	@rm -rdf $(OBJ)
	@make -C lib/libft fclean
	@make -C lib/get_next_line fclean
	@printf $(GREEN)"\r\033[KObjects cleaned üóë"$(RESET)
	@printf $(CYAN)"\r\033[KErasing binary file... "$(RESET)"‚è≥ "
	@rm -rdf $(NAME) test_parser
	@printf $(GREEN)"\r\033[KBinary file cleaned üóë"$(RESET)
	@printf $(GREEN)"\r\033[KForce cleaned üóë"$(RESET)
	@vlc includes/videoplayback.mp4
	@clear

# Clean readline
fclean_readline:
	@printf $(CYAN)"\r\033[KErasing readline... "$(RESET)"‚è≥ "
	@rm -rdf vendor
	@printf $(GREEN)"\r\033[KReadline cleaned üóë"$(RESET)

# Clean all and recompile minishell
re: fclean all

.PHONY: all clean fclean re load fclean_readline run readline drucker